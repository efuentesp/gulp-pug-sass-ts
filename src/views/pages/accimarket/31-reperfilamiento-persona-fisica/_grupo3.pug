include /mixins/mixins

+field-group("grupoTres")
    .amRow.justify-end#divGrupoParrafo
        p.font-bold.pr-40 Si conoce y ha invertido, indique:
    .amRow
        +quiz("encuesta", {name: "2. ¿Cuál es la experiencia en productos de inversión del cliente?", size:"40em"}, [], [
        {answer: "No conoce ni ha invertido", type: "option"},
        {answer: "Sólo conoce", type: "option"},
        {answer: "Conoce y ha invertido", type: "option"},
        {answer: "Frecuencia", type: "select", size: "12em", disabled: true},
        {answer: "Plazo", type: "select", size: "10em", disabled: true},
        {answer: "Volumen", type: "select", size: "12em", disabled: true}
    ])

